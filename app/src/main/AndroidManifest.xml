<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="hackathon.ote.gr.otehackathon">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Application"
        android:allowBackup="true"
        android:icon="@drawable/cosmote_small_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cosmote_small_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.FormActivity">

        </activity>
        <activity
            android:name=".activities.SymtpmsListActivity"
            android:label="@string/title_activity_symtpms_list"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>